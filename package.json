    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "mammoth": "^1.11.0",
    "prettier": "^3.2.5",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.1.0",
